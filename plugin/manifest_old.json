{
	"name": "talkn",
	"version": "0.1",
	"manifest_version": 2,
	"description": "CHAT ON WEB SITE",
	"permissions": [ "storage", "http://*.talkn.io/*", "https://*.talkn.io/" ],
	"background": {
		"scripts":[ "background.js" ],
		"presistent": false
	},
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"js": [ "talkn.client.js" ],
			"run_at": "document_start"
		}
	],
	"web_accessible_resources": [ "js/*", "css/*", "img/*", "html/*" ]
}
