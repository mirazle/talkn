<style type="text/css">
    body {
        color: rgb(255,255,255);
        margin: 0;
        /* font-family: 游ゴシック, YuGothic, "Hiragino Sans", "Hiragino Kaku Gothic Pro", メイリオ, Meiryo, sans-serif; */
        font-family: "Hiragino Kaku Gothic ProN","メイリオ", sans-serif;
        letter-spacing: 0.03em;
        font-size: 3vw;
    }

    .fixBackground {
        position: fixed;
        top: 0;
        left: 0;
        z-index: 0;
        width: 100%;
        height: 100%;
    }

    .background {
        position: absolute;
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
        z-index: -1;
        height: 0;
        width: inherit;
        height: inherit;
        background: url("//<%= assetsURL %>/www/img1.jpg") center center no-repeat;
        background-size: cover;
        overflow: hidden;
    }

    .container {
        position: relative;
        display: flex;
        align-items: center;
        justify-content: center;
        flex-direction: column;
        flex-wrap: wrap;
        margin: 0;
        width: auto;
        height: auto;
        overflow: hidden;
        z-index: 2;
    }

    section {
        display: flex;
        align-items: center;
        justify-content: center;
        flex-direction: column;
        box-sizing: border-box;
        width: 100%;
        height: 960px;
    }

    h2 {
        font-size: 1.3em;
        letter-spacing: <%= letterSpacingMiddle %>
    }

    h3 {
        padding: 1vw 0px 0px 0px;
        margin: 0;
    }

    h4 {
        font-size:0.75em;
    }

    header {
        display: flex;
        align-items: center;
        justify-content: center;
        flex-direction: column;
        width: 100%;
        height: 100px;
        z-index: 10;
        font-size: 15px;
    }

    header .language-menu {
        box-sizing: border-box;
        position: absolute;
        top: -380px;
        display: flex;
        justify-content: flex-start;
        align-items: flex-start;
        flex-direction: column;
        flex-wrap: wrap;
        background: rgba(0,0,0,0.8);
        padding: 10px;
        width: 100%;
        height: 380px;
        border-radius: 0px 0px 5px 5px;
        z-index: 1;
        transition: 300ms;
        transform: translate3d(0px, 0px, 0px);
    }

    header .language-menu li {
        display: inline-block;
        box-sizing: border-box;
        color: rgba(255,255,255,0.9);
        margin: 2px;
        padding: 10px;
        width: 30%;
        height: 40px;
        transition: 300ms;
    }

    header .language-menu li.active:hover {
        color: rgba(0,0,0,0.8);
        background: rgba(255,255,255,0.8);
    }

    .menus, nav {
        display: flex;
        align-items: center;
        justify-content: center;
        flex-direction: column;
        width: 100%;
        margin: 0;
        padding: 0;
    }

    .headBar {
        position: absolute;
        top: 0px;
        width: 100%;
        min-height: 5px;
        background-color: rgba(0,0,0,0.9);
    }

    nav {
        position: absolute;
        top: 45px;
        width: inherit;
        height: 55px;
        background: rgba(255,255,255,0.9);
        color: rgb(100, 100, 100);
        box-sizing: border-box;
        border-bottom: 1px solid rgba(0,0,0,0.1)
    }

    nav ul {
        position: absolute;
        top: -5px;
        max-width: 800px;
        min-width: 320px;
    }
    
    .menus {
        position: absolute;
        top: 0px;
        flex-direction: row;
        height: 45px;
        background: rgba(0,0,0,0.8);
        z-index: 1;
    }

    ul, .pointerWrap {
        display: flex;
        justify-content: space-between;
        width: inherit;
        height: 70px;
        max-width: 800px;
        min-width: 320px;
        margin: 0;
        padding: 0;
    }

    nav li, .menus li, footer li {
        display: inline-flex;
        align-items: center;
        justify-content: center;
        width: inherit;
        height: inherit;
        margin: 0;
        padding: 0;
        cursor: pointer;
    }

    nav li {
        
    }

    .selectMenuWrap {
        position: fixed;
        top: -27px;
        display: flex;
        align-items: center;
        justify-content: center;
        width: 50px;
        height: 50px;
        background: rgb(0,0,0,0.7);
        border-radius: 100px;
    }

    .pointerWrap {
        position: absolute;
        height: initial;
        top: 43px;
        z-index: 0;
    }

    .pointer{
        width: calc( 10% );
        margin: 0px 7.5% 0px 7.5%;
        height: 5px;
        border-radius: 10px;
        background: rgb(79, 174,159);
        color: rgb( 255, 255, 255 );
        z-index: 1;
        transition: 300ms;
        transform: translate3d( 0px, 0px, 0px );
    }

    .accessory {
        display: flex;
        justify-content: center;
        align-items: flex-end;
        width: 55px;
        height: 55px;
        line-height: 35px;
        background: rgba(0,0,0,1);
        position: fixed;
        top: -25px;
        border-radius: 100px;
        color: rgb(255,255,255);
        font-size: 8px;
        transition: 300ms;
        transform: translate3d(0px,-25px,0px);
    }

    .accessory-case {
        position: fixed;
        top: -100px;
        width: 100%;
        height: 100px;
        background: rgb(255,255,255);
    }

    section.one ::placeholder {
        font-size: 14px;
        line-height: 14px;
        letter-spacing: 2px;
        color: rgba( 100,100,100 );
        text-indent: 3vw;
    }
    

    section.one input:hover, section.one input:focus {
        color: rgba( 255,255,255 );
        background: rgba(79, 174, 159, 0.7);
    }

    section.one ::placeholder {
        color: rgba( 255,255,255 );
    }

    section.one input {
        width: inherit;
        height: 40px;
        font-size: 14px;
        letter-spacing: 2px;
        box-sizing: border-box;
        border: 0;
        border-radius: 20px;
        padding: 10px 50px 10px 10px;
        background: rgba(55,55,55,0.5);
        outline: 0;
        text-indent: 3vw;
        line-height: 14px;
        box-shadow: 0px 0px 0px 4px rgba(255,255,255,0.9);
        transition: 300ms;
    }

    section .content {
        display: flex;
        justify-content: center;
        align-items: center;
        flex-direction: row;
        position: absolute;
        width: 100%;
    }

    section .title {
        position: relative;
        display: flex;
        align-items: center;
        justify-content: center;
        flex-direction: column;
        height: 120px;
        width: 100%;
    }

    section .under-bar {
        position: relative;
        height: 4px;
        margin: 13px 0px 0px 0px;
        border-radius: 30px;
    }

    section.one{
        flex-direction: column;
    }

    section.one .content {
        display: flex;
        align-items: center;
        justify-content: center;
        flex-direction: column;
        width: 100%;
    }

    section.one .eyecatch {
        display: flex;
        align-content: center;
        justify-content: center;
        flex-direction: column;
        flex-wrap: wrap;
        width: 100%;
        margin: 0px 0px 100px 0px;
    }

    section.one .subtitle {
        width: 100%;
        text-align: center;
        text-shadow: 0px 0px 3px rgb(0,0,0);
    }

    section.one h2 {
        margin: 0;
        text-align: center;
        color: rgba(255, 255, 255, 1);
        font-size: 38px;
        text-shadow:
            1px 1px 5px rgb(0,0,0),
            -10px -10px 50px rgb(255,255,255),
            10px 10px 50px rgb(255,255,255),
            10px 10px 50px rgb(255,255,255);
    }

    section.one form {
        display: flex;
        align-items: center;
        justify-content: center;
        width: 70%;
        margin: 0px 0px 100px 0px;
    }

    section.one .inputWrap {
        min-width: 300px;
        width: 86%;
        max-width: 600px;
        margin: 0px 0px 130px 0px;
    }

    section.one .inputWrap {
        display: flex;
        align-items: center;
        justify-content: center;
        flex-direction: column;
    }

    section.one .searchIcon {
        display: flex;
        align-items: center;
        justify-content: flex-end;
        position: relative;
        top: 34px;
        left: 15px;
        height: 32px;
        z-index: 0;
        margin: 0px 0px 0px 70%;
    }

    section.one .searchIcon .circle{
        overflow: hidden;
        width: 16px;
        height: 16px;
        padding: 0px;
    }