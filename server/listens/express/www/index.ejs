        top: -25px;
        border-radius: 100px;
        color: rgb(255,255,255);
        transition: 300ms;
        transform: translate3d(0px,-25px,0px);
    }

    .accessory-case {
        position: fixed;
        top: -100px;
        width: 100%;
        height: 100px;
        background: rgb(255,255,255);
    }

