<script type="text/javascript">
    const responsiveWidth = 640;
    const headerHeight = 100;
    const pointerStyles = [
        {width: "10%", transform: "translate3d(0%,0px,0px)"},
        {width: "10%", transform: "translate3d(250%,0px,0px)"},
        {width: "10%", transform: "translate3d(500%,0px,0px)"},
        {width: "10%", transform: "translate3d(750%,0px,0px)"}
    ];
    const menuStyles = {
        active: {color: "rgb(79, 174, 159)"},
        unactive: {color: "rgb(55, 55, 55)"},
    }
    let screen = 0;
    let isAnimateScrolling = false;
    let oneScreenHeight = 600;

    function onMouseOver(num){
        movePointer(num);
        setMenuColor();
    }

    function onMouseLeave(num){
        movePointer(screen);
        setMenuColor();
    }

    function onClick(num){
        setScreen(num);
        movePointer(num, true);
        setMenuColor();
    }

    function getCalcScreen(){
        const { scrollY, innerHeight } = window;
        if( 0 <= scrollY && scrollY < ( oneScreenHeight * 1 ) + headerHeight) {
            screen = 0;
        }else if( oneScreenHeight + headerHeight <= scrollY && scrollY < ( oneScreenHeight * 2 ) + headerHeight ){
            screen = 1;
        }else if( ( oneScreenHeight * 2 ) + headerHeight <= scrollY && scrollY < ( oneScreenHeight * 3 ) + headerHeight ){
            screen = 2;
        }else if( ( oneScreenHeight * 3 ) + headerHeight <= scrollY ){
            screen = 3;
        }
        return screen;
    }

    function setScreen(num){
        if(num){
            screen = num;
        }else{
            screen = getCalcScreen();
        }
        return screen;
    }

    function setMenuColor(){
        if( window.menuLis ){
            window.menuLis.forEach( (menuLi, i) => {
                menuLi.style.color = screen === i ? menuStyles.active.color : menuStyles.unactive.color;
            } );
        }
    }

    function movePointer(num = 0, animateScroll = false){
        const { scrollY, innerHeight } = window.scrollY;
        const pointerStyle = pointerStyles[ num ];
        const pointer = document.querySelector(".pointer");
        pointer.style.width = pointerStyle.width;
        pointer.style.transform = pointerStyle.transform;
        const moveScrollY = num === 0 ? 0 : ( oneScreenHeight * num ) + headerHeight ;
        if( animateScroll ){
            animateScrollTo( moveScrollY, 600 );
        }
    }

    function setNavStyles(){
        const nav = document.querySelector("nav");
        const accessory = document.querySelector(".accessory");
        const scrollY = window.scrollY;

        if( scrollY < 45 && nav.style.position === "fixed" ){
            document.querySelector("nav").style.position = "absolute";
            document.querySelector("nav").style.top = "45px";
            document.querySelector(".accessory").style.transform = "translate3d(0px, -25px, 0px)";
        }else if(scrollY >= 45 && nav.style.position === "absolute"){
            document.querySelector("nav").style.position = "fixed";
            document.querySelector("nav").style.top = "0px";
            document.querySelector(".accessory").style.transform = "translate3d(0px, 0px, 0px)";
        }else if( nav.style.position === "" ){
            document.querySelector("nav").style.position = "absolute";
            document.querySelector("nav").style.top = "45px";
            document.querySelector(".accessory").style.transform = "translate3d(0px, -25px, 0px)";
        }
    }

	function animateScrollTo( to = 99999, duration = 400,  callback = () => {} ){

        if( duration === 0 ){
            window.scrollTo(0, to);
        }else{

            if( !isAnimateScrolling ){
                let start = window.scrollY;
                let change = to - start;
                let currentTime = 0;
                let increment = 20;

                const animateScroll = ()　=>　{
                    currentTime += increment;
                    let scrollTop = Math.easeInOutQuad(currentTime, start, change, duration);
                    screenTop = Math.floor( scrollTop );
                    window.scrollTo(0, scrollTop);

                    if(currentTime < duration){
                        isAnimateScrolling = true;
                        setTimeout(animateScroll, increment);
                    }

                    if( to === screenTop){
                        isAnimateScrolling = false;
                        callback();
                    }
                };
                animateScroll();
            }
        }
    }

    function setupMath(){
		Math.easeInOutQuad = function (t, b, c, d) {
			t /= d/2;
			if (t < 1) return c/2*t*t + b;
			t--;
			return -c/2 * (t*(t-2) - 1) + b;
		};
	}

    function onMouseOverInput( className ){
        document.querySelector("section." + className ).style.border = "5px solid rgba( 79,174,159,0.8)";
    } 

    function onMouseLeaveInput( className ){
        document.querySelector("section." + className ).style.border = "5px solid rgba( 0,0,0,0.2)";
    } 

    function onFocusInput( className ){
        document.querySelector("section." + className ).style.border = "5px solid rgba( 79,174,159,0.8)";
    } 

    function onBlurInput( className ){
        document.querySelector("section." + className ).style.border = "5px solid rgba( 0,0,0,0.2)";
    } 

    function onResizeWindow(){
        let newOneScreenHeight = 0;
        if( window.innerWidth < responsiveWidth ){
            newOneScreenHeight = 600;
        }else{
            newOneScreenHeight = 960;
        }

        if( oneScreenHeight !== newOneScreenHeight ){
            oneScreenHeight = newOneScreenHeight;
            screen = setScreen();
            movePointer(screen);
            setNavStyles();
            setMenuColor();
        }
    } 

    window.onload =  ( e ) => {
        onResizeWindow();
        setupMath();
        window.pointer = document.querySelector(".pointer");
        window.menuLis = document.querySelectorAll("nav li");
        window.nav = document.querySelector("nav");
        setNavStyles();
        setMenuColor();
    };

    window.onscroll =  ( e ) => {

        setNavStyles();

        if( !isAnimateScrolling ){

            let beforeScreen = screen;
            screen = setScreen();

            if( screen !== beforeScreen){
                movePointer(screen);
                setMenuColor();
            }
        }
    };

    window.onresize = onResizeWindow;

</script>

