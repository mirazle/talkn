            document.querySelector( selector ).style.opacity = "1";
            const copyTag = document.createElement( "input" );
            copyTag.value = document.querySelector( `${ selector } pre` )
                .innerText
                .replace( `// ${ replaceComment }`, "" )
                .replace( /\r?\n/g, '' )
                .replace( /    /g, " " );
            copyTag.id = type;
            copyTag.type = "text";
            copyTag.style = "opacity: 0";
            document.querySelector( selector ).appendChild( copyTag );
            copyTag.focus();
            copyTag.select();
            document.execCommand( "copy" );
            setTimeout( () => {
                document.querySelector( selector ).style.opacity = "0";
                setTimeout( () => {
                    copyTag.parentNode.removeChild( copyTag );
                }, 1000 );
            }, 3000 );
        }
        function onResizeWindow() {
            let newOneScreenHeight = 0;
            if ( window.innerWidth < responsiveWidth ) {
                newOneScreenHeight = smallScreenWidth;
            } else {
                newOneScreenHeight = wideScreenWidth;
            }
