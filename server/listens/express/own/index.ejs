        function getCalcScreen() {
            const { scrollY, innerHeight } = window;
            if ( 0 <= scrollY && scrollY < ( oneScreenHeight * 1 ) + headerHeight ) {
                screen = 0;
            } else if ( oneScreenHeight + headerHeight <= scrollY && scrollY < ( oneScreenHeight * 2 ) + headerHeight ) {
                screen = 1;
            } else if ( ( oneScreenHeight * 2 ) + headerHeight <= scrollY && scrollY < ( oneScreenHeight * 3 ) + headerHeight ) {
                screen = 2;
            } else if ( ( oneScreenHeight * 3 ) + headerHeight <= scrollY && scrollY < ( oneScreenHeight * 4 ) + headerHeight ) {
                screen = 3;
            } else if ( ( oneScreenHeight * 4 ) + headerHeight <= scrollY ) {
                screen = 4;
            }
            return screen;
        }
        function setScreen( num ) {
            if ( num ) {
                screen = num;
