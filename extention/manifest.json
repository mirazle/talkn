{
    "manifest_version": 2,
    "name": "talkn",
    "description": "talkn can share comments with users watching the same WEB page. Please enjoy the world of talkn.",
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": [ "contentScript.js" ],
        "run_at": "document_end"
      }
    ],
    "web_accessible_resources": [
      "index.html",
      "include.js",
      "talkn.client.js"
    ],
    "permissions": [
      "storage"
    ],
    "version": "0.675"
}
