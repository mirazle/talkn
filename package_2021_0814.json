  "scripts": {
    "dev": "yarn run server & yarn run client & yarn run api",
    "server": "nodemon -L",
    "devServer": "nodemon -L",
    "api": "WEBPACK_ENV=development MODE=api webpack-cli serve --hot --inline --https --cert=./common/pems/clients/localhost.crt --key=./common/pems/clients/localhost.key",
    "client": "WEBPACK_ENV=development MODE=client webpack-cli serve --hot --inline --https --cert=./common/pems/clients/localhost.crt --key=./common/pems/clients/localhost.key",
    "build": "WEBPACK_ENV=production MODE=client webpack; WEBPACK_ENV=production MODE=api webpack;",
    "prod": "forever start -c 'ts-node -r tsconfig-paths/register' server/run.ts",
    "stop": "forever stopall"
  },
